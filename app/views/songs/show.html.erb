
    <table>
        <thead>
            <tr>
                <th>Song Title</th>
                <th>release_year</th>
                <th>released</th>
                <th>genre</th>
                <th>artist_name</th>

            </tr>
        </thead>
        <tbody>
            <tr>
                <td><%= @song.title %></td>
                <td><%= @song.release_year %></td>
                <td><%= @song.released %></td>
                <td><%= @song.genre %></td>
                <td><%= @song.artist_name %></td>

            </tr>
        </tbody>
    </table>
    <%= form_tag song_path(@song.id), method: "delete" do %>
        <%= submit_tag "Delete #{@song.title}" %>
      <% end %>
    <%= link_to "all songs", controller: "songs", action: "index" %>
    <%= link_to "edit song", controller: "songs", action: "edit" %>